[#include parts/header]

<div class="wrapper">
    <div class="sidebar" data-color="white" data-active-color="danger">
      	<div class="logo">
			<a href="/[language]/schedule/index" class="simple-text logo-mini">
				<div class="logo-image-small">
					<img src="/img/cabbage.svg">
				</div>
			</a>
			<a href="/[language]/schedule/index" class="simple-text logo-normal">
				[mainName]
			</a>
		</div>

		<div class="sidebar-wrapper">
			<ul class="nav">
				<li class="active">
					<a href="/[language]/schedule/index">
						<i class="nc-icon nc-chart-bar-32"></i>
						<p>მთავარი</p>
					</a>
				</li>

				<li>
					<a href="/[language]/home/index">
						<i class="nc-icon nc-button-power"></i>
						<p>გასვლა</p>
					</a>
				</li>
			</ul>
		</div>
	</div>

	<div class="main-panel">
		<nav class="navbar navbar-expand-lg navbar-absolute fixed-top navbar-transparent">
			<div class="container-fluid">
				<div class="navbar-wrapper">
					<div class="navbar-toggle">
						<button type="button" class="navbar-toggler">
							<span class="navbar-toggler-bar bar1"></span>
							<span class="navbar-toggler-bar bar2"></span>
							<span class="navbar-toggler-bar bar3"></span>
						</button>
					</div>
					<a href="/[language]/schedule/index" class="navbar-brand">[title]</a>
				</div>

				<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navigation" aria-controls="navigation-index" aria-expanded="false" aria-label="Toggle navigation">
					<span class="navbar-toggler-bar navbar-kebab"></span>
					<span class="navbar-toggler-bar navbar-kebab"></span>
					<span class="navbar-toggler-bar navbar-kebab"></span>
				</button>

				<div class="collapse navbar-collapse justify-content-end" id="navigation">

					<ul class="navbar-nav">
						<li class="nav-item">
							<a class="nav-link dropdown-toggle" href="/" id="navbarDropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
								<i class="nc-icon nc-settings-gear-65"></i>
								<p>
									<span class="d-lg-none d-md-block">ანგარიში</span>
								</p>
							</a>
							<div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdownMenuLink">
								<a class="dropdown-item" href="/[language]/home/index">სისტემიდან გასვლა</a>
							</div>
							
						</li>
					</ul>
				</div>
			</div>
		</nav>
		
		<div class="content">
			<div class="row">
				
				<div class="col-md-12">
					<div class="card">
						<div class="card-header">
							<h4 class="card-title" style="display: inline-block;"> გადახდის გრაფიკი</h4>
						</div>
						
						<div class="card-body">
							ტესტინგი
						</div>
					</div>
				</div>

			</div>
		</div>

		[#include parts/topFooter]
	</div>
</div>


[#include parts/footer]