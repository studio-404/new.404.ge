[#include parts/header]

<div class="wrapper">
    <div class="sidebar" data-color="white" data-active-color="danger">
      	<div class="logo">
			<a href="/" class="simple-text logo-mini">
				<div class="logo-image-small">
					<img src="/img/logo-small.png">
				</div>
			</a>
			<a href="/[language]/dashboard/index" class="simple-text logo-normal">
				[mainName]
			</a>
		</div>


		[#include parts/leftNav]
	</div>

	<div class="main-panel">
		[#include parts/topNav]
		
		<div class="content">
			<div class="row">
				<div class="col-md-12" style="margin-top: 0; margin-bottom: 20px;">
					<div style="display: inline-block;">
						<a href="[language]/dashboard/index" style="display: inline-block;">მთავარი</a> &#8594; 
						<a href="/[language]/building/index" style="display: inline-block;">მშენებლობა</a> &#8594; 
						<a href="/[language]/entrance/index/[building_id]" style="display: inline-block;">სადარბაზო</a>
					</div>
				</div>

				<div class="col-md-12">
					<div class="card">
						<div class="card-header">
							<h4 class="card-title" style="display: inline-block;"> სადარბაზო</h4>
							<div class="update ml-auto mr-auto" style="float:right">
		                      <a href="/[language]/entrance/add/[building_id]" class="btn btn-primary btn-round">დამატება</a>
		                    </div>

							<div class="update ml-auto mr-auto" style="float:right">
		                      <a href="/[language]/building/index" class="btn btn-primary btn-round">უკან</a>
		                    </div>
						</div>
						
						<div class="card-body">
							[entranceList]
						</div>
					</div>
				</div>
			</div>


		</div>

		[#include parts/topFooter]
	</div>
</div>

<script type="text/javascript">
var Config = {
	ajax:"/[language]/ajax",
	language:"[language]"
};
</script>
<script src="/js/entrance.js" type="text/javascript" charset="utf-8"></script>

[#include parts/footer]